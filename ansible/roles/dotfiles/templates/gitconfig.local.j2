# ~/.gitconfig.local
# Machine-specific git configuration. This file stays out of version control.
# Generated by setup-git-config.sh

[user]
	name = {{ git_user_name | default('Your Name') }}
	email = {{ git_user_email | default('your.email@example.com') }}
{% if git_signing_key is defined and git_signing_key %}
	signingkey = {{ git_signing_key }}
{% endif %}
