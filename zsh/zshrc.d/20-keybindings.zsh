# Keybindings for menu select
bindkey -M menuselect "^[OD"   .backward-char # ←
bindkey -M menuselect "^[OC"    .forward-char # →
bindkey -M menuselect "^[b"    .backward-word # Alt + ←
bindkey -M menuselect "^[f"     .forward-word # Alt + →
bindkey -M menuselect "^A" .beginning-of-line # Ctrl + ←
bindkey -M menuselect "^E"       .end-of-line # Ctrl + →
